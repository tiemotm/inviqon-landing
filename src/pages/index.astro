---
import BaseLayout from "../layouts/BaseLayout.astro";
import Hero from "../components/Hero.astro";
import Footer from "../components/Footer.astro";
import Studio from "../components/Studio.astro";
import Work from "../components/Work.astro";
import Process from "../components/Process.astro";
import CTA from "../components/CTA.astro";

const seo = {
  title: "inviqon — We build the digital products of tomorrow",
  description:
    "inviqon is a software house & product studio. We build digital products from 0→1 and take on selected engagements — with design intent and engineering discipline.",
};

const brand = {
  name: "inviqon",
  email: "hello@inviqon.com"
}
---

<BaseLayout title={seo.title} description={seo.description}>
  <Hero />
  <Studio />
  <Work />
  <Process />
  <CTA email={brand.email} />
  <Footer />
</BaseLayout>